using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            try
            {
                int a,b,c;
                a = 5;
                b = 0;
                c = a / b;
            }
            catch(Exception e)
            {
                Console.WriteLine("THE ERROR IS:"+e);
            }
            finally
            {
                Console.WriteLine("THE Exception HANDLING");
            }
            Console.ReadLine();
        }
    }
}
